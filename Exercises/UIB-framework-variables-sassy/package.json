{
  "name": "sassy",
  "version": "1.0.0",
  "description": "Fran's stuff",
  "main": "index.html",
  "author": "Fran",
  "homepage": "https://itsanytime.github.io/DCI_Exercises/Exercises/UIB-framework-variables-sassy/src/",
  "scripts": {
    "start": "run-p watch watch:styles",
    "build": "run-s clean clean:styles build:styles copy",
    "predeploy": "npm run UIB-framework-variables-sassy",
    "deploy": "gh-pages -d UIB-framework-variables-sassy",
    "build:styles": "sass src/scss:src/styles",
    "watch": "live-server src",
    "watch:styles": "sass src/scss:src/styles --watch",
    "clean": "rm -rf dist",
    "clean:styles": "rm -rf src/styles",
    "copy": "mkdir dist && rsync -avr --exclude=\"/scss\" src/ dist",
    "publish": "gh-pages -d dist"
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^4.3.1"
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "lodash": ">=4.17.19",
    "websocket-extensions": ">=0.1.4",
    "kind-of": ">=6.0.3",
    "sass": "^1.23.0-module.beta.1"
  }
}
